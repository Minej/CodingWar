<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapp5">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/CodingWars"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp5">
        <activity android:name=".ZadaniyaActivity"></activity>
        <activity android:name=".ChatUserActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".FriendActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".ViewFriendActivity" />
        <activity android:name=".FindFriendActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SetupActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".Utills.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".MainActivity" />
    </application>

</manifest>